{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "routes": [
    {
      "route": "*",
      "methods": [
        "get",
        "head",
        "post",
        "put",
        "connect",
        "options",
        "trace",
        "patch"
      ],
      "redirect": "/index.html",
      "statusCode": 301
    }
  ]
}
